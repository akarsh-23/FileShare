{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "/upload",
            "allowedRoles": [
                "authenticated"
            ]
        },
        {
            "route": "/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/.auth/login/google?post_login_redirect_uri=.referrer"
        }
    },
    "auth": {
        "rolesSource": "/api/roles",
        "identityProviders": {
            "google": {
                "registration": {
                    "clientIdSettingName": "GOOGLE_CLIENT_ID",
                    "clientSecretSettingName": "GOOGLE_CLIENT_SECRET_APP_SETTING_NAME"
                }
            }
        }
    }
}